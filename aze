<?php
class Question{
    private string $RefQ;
    private string $TitreQ;
    private string $DesQ;
    private string $ContenuQ;

    
    public function  __construct ($RQ,$TQ,$DQ,$CQ){
        $this->RefQ=$RQ;
        $this->TitreQ=$TQ;
        $this->DesQ=$DQ;
        $this->ContenuQ=$CQ;
 
       
    }
    public function get_RefQ()
    {
        return $this->RefQ;
    }
    public function get_TitreQ()
    {
        return $this->TitreQ;
    }   
    public function get_DesQ()
    {
        return $this->DesQ;
    }
    public function get_CQ()
    {
        return $this->ContenuQ;
    }


    
}
?>
